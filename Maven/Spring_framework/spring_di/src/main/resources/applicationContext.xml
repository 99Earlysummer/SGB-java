<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd">
<!--构造器方法注入-->
<bean id="user" class="com.sgb.pojo.User">
        <!--DI注入 简单类型  声明student对象-->
        <constructor-arg name="userName" value="沈功波"></constructor-arg>
        <constructor-arg name="userId" value="111"></constructor-arg>
        <constructor-arg name="userPwd" value="1314"></constructor-arg>
        <!--DI注入 javabean  声明student对象-->
        <constructor-arg name="student" ref="student"></constructor-arg>
</bean>

                            <bean id="student" class="com.sgb.pojo.Student">
                                <constructor-arg name="name" value="学生"></constructor-arg>
                                <constructor-arg name="age" value="111"></constructor-arg>
                            </bean>
<!--set方法注入-->
<bean id="user2" class="com.sgb.pojo.User">
    <!--DI注入 简单类型  声明student对象-->
    <property name="userName" value="这衣服"></property>
    <property name="userId" value="222"></property>
    <property name="userPwd" value="520"></property>
    <!--DI注入 javabean  声明student对象-->
    <property name="student" ref="student2"></property>
</bean>
                            <bean id="student2" class="com.sgb.pojo.Student">
                                <constructor-arg name="name" value="学生2"></constructor-arg>
                                <constructor-arg name="age" value="212"></constructor-arg>
                            </bean>

<!--容器注入 (property-array-value)-->
<bean id="bean01" class="com.sgb.pojo.Bean01">
    <property name="strs">
        <array>
            <value>a</value>
            <value>b</value>
            <value>c</value>
        </array>
    </property>
    <property name="strList">
        <list>
            <value>a</value>
            <value>b</value>
            <value>c</value>
        </list>
    </property>
    <property name="studentList">
        <list>
            <bean id="mystu1" class="com.sgb.pojo.Student"></bean>
            <bean id="mystu2" class="com.sgb.pojo.Student"></bean>
            <ref bean="student"></ref>
            <ref bean="student2"></ref>
        </list>
    </property>
    <property name="strSet">
        <set>
            <value>a</value>
            <value>b</value>
            <value>c</value>
        </set>
    </property>
    <property name="map">
        <map>
            <entry key="1" value="a"></entry>
            <entry key="2" value="b"></entry>
            <entry key="3" value="c"></entry>
        </map>
    </property>
    <property name="properties">
        <props>
            <prop key="1">a</prop>
            <prop key="2">b</prop>
            <prop key="3">c</prop>
        </props>
    </property>
</bean>



<!--p命名空间注入-->
<bean id="user3" class="com.sgb.pojo.User"
     p:userId="13"
     p:userName="沈功波"
     p:userPwd="14"
     p:student-ref="student3">
</bean>
                    <bean id="student3"
                          class="com.sgb.pojo.Student"
                          p:age="31"
                          p:name="阿三">
                    </bean>
</beans>